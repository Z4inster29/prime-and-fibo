function isPrime(num){
  for (var i = 2; i< num; i++){
    
    if ( num % i == 0){
  return false;
  }
}
return true;
}
for (var i = 2; i< 1000; i++){
  if (isPrime(i) == true){
    console.log(i);
    document.write(i);
    document.write("<br/>");
  }
}

console.log(isPrime(31));
console.log(isPrime(24));


